[ "${OSTYPE:-`uname`}" = "${BASH_SOURCE#*.}" ] || return 0

PATHSEP=':'

#TODO support Debian/WSL?
is_exec yum && {
alias yum.vars="python -c 'import yum, pprint; yb = yum.YumBase(); pprint.pprint(yb.conf.yumvar, width=1)'"
}

source ${BASH_SOURCE/bashrc/functions} || return

