# $Id$

_bash_source=${BASH_SOURCE##*/}
[ "${OSTYPE:-`uname`}" = "${_bash_source#?*.}" ] || return 0

# force symlinks otherwise MINGW resorts to copying files!
export MSYS=winsymlinks:nativestrict

alias ps='\ps -f'
GIT_PROMPT=1

# path-handling is not 'space'-safe
unset EDITOR

